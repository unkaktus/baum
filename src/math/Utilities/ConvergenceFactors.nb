Cell[BoxData[
    \(\(\( (*\ 
      grid\ size\ varying\ with\ level\ *) \)\(\[IndentingNewLine]\)\(\
\[IndentingNewLine]\)\(sizeoflevel[lmax_, hmax_, nnn_]\  := \ 
      Module[{s, t}, \[IndentingNewLine]nn = 
          Table[nnn[\([i]\)] + 12, {i, 1, 
              Length[nnn]}]; \[IndentingNewLine]s = 
          Sum[nn[\([i]\)]^3, {i, 1, 
              Length[nn]}]; \[IndentingNewLine] (*\(\(+Sum[
                nn[\([i]\)]^3, {i, Length[nn] - 2, 
                  Length[nn]}]\);\)*) \[IndentingNewLine]t = 
          Table[If[i <= lbo, 1, 2^\((i - lbo)\)], {i, 0, 
              lmax}]; \[IndentingNewLine]m = 
          Table[Round[1000.0*nn[\([i + 1]\)]^3/s]/10.0, {i, 0, 
              lmax}]; \[IndentingNewLine]stm = 
          Sum[m[\([i]\)] t[\([i]\)], {i, 1, 
              Length[t]}]; \[IndentingNewLine]Print[
          TableForm[
            Join[{{"\<level\>", "\<n\>", "\<mem\>", "\<steps\>", "\<flops\>", \
"\<h\>", "\<r\>"}}, \[IndentingNewLine]Table[{i, \[IndentingNewLine]nn[\([i + 
                        1]\)], \[IndentingNewLine]m[\([i + 
                        1]\)], \[IndentingNewLine]t[\([i + 
                        1]\)], \[IndentingNewLine]Round[
                      1000\ m[\([i + 1]\)] t[\([i + 1]\)]/stm]/
                    10. , \[IndentingNewLine]hmax/2.0^i, 
                  nn[\([i + 1]\)]\ hmax/
                      2.0^\((i + 1)\)}, \[IndentingNewLine]{i, 0, 
                  lmax}]\[IndentingNewLine]]]]; \
\[IndentingNewLine]Print["\<points: \>", s, "\<  flops: \>", \ 
          Sum[nn[\([i + 1]\)]^3  t[\([i + 1]\)], {i, 0, 
              lmax}]];\[IndentingNewLine]]\[IndentingNewLine]\
\[IndentingNewLine]
    \(n\  = \ 40;\)\[IndentingNewLine]
    \(lbo = 2;\)\[IndentingNewLine]
    sizeoflevel[8, 
      6.4, {2  n, 2  n, 2  n, 2  n, n, n, n, n, n}]\)\)\)], "Input"],

Cell[BoxData[
    TagBox[GridBox[{
